{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "app.py",
        "GOOGLE_SHEETS_PRIVATE_KEY": "",
        "GOOGLE_SHEETS_CLIENT_EMAIL": "",
        "GOOGLE_SHEET_NAME": "",
        "GOOGLE_SHEET_ID": "",
        "SECRET_KEY": ""
    }
} 